set wildmenu
set hlsearch
set autoindent
set nostartofline
set path+=**
set wildignore+=*/node_modules/*,*/__pycache__/*,*/.expo/*,*/.expo-shared/*
set number relativenumber
set backspace=indent,eol,start
set tabstop=2 shiftwidth=2 softtabstop=0 smarttab
filetype plugin indent on

packloadall
syntax enable
colorscheme dracula
